{
	"query": "str", // Consulta
	"status": "str", // status do resultado { "OK", "ERROR" }
	"from": "str", // string da data de início da CONSULTA ('%d-%m-%Y')
	"to": "str", // string da data de fim da CONSULTA ('%d-%m-%Y')
	"stats": {
		"nunique_docs": "int", // Quantidade de documentos únicos usados para gerar o resulta
		"nqueries": "int", // quantidade de requisições de consulta
		"ndocs": "int", // quantidade de documentos usados no processamento
		"ndomains": "int", // quantidade de domínios retornados
		"time": {
			"total": "str", // tempo em segundos totais gastos para gerar o resultado
			"query": "str", // tempo em segundos gastos na consulta 
			"total": "str" // tempo em segundos gastos para gerar as keywords [ tem outros detalhes mais de tempo para cada fase ]
		}
	},
	"domains": [ "str" ], // Nome dos domínios recuperados na consulta
	"query_term_corr": [ ["str", "float"] ], // par de termos mais correlatos com a consulta "query" e seus respectivos pesos
	"sentiment": {
		"raw": [ [ "int", "int", "str" ] ], // sentimentos dos documentos processados na consulta.
											// A tripla é dada como:
												// id do domínio (mesmo id do array domains);
												// sentimento (-1=negativo,0=neutro,1=positivo);
												// date: data do documento que ocorre determinado resultado
		"all": { "int": "int" } // Sumarização dos resultados. Contagem dos sentimentos.
	},
	"results": [ {
			"from": "str", // data de início do intervalo
			"to": "str",   // data de fim do intervalo
			"ndocs": "int", // quantidade de documentos no intervalo
			"from_all_keys": [ ["str","float","int"] ], // resultados para todos domínios e todos sentimentos
												// string do keyphrase;
												// peso do keyphrase (menos é melhor);
												// sentimento da keyphrase (-1=negativo,0=neutro,1=positivo)
			"positive": [ ["str","float","int"] ], // resultados para todos domínios e com sentimento positivo
												// string do keyphrase;
												// peso do keyphrase (menos é melhor);
												// sentimento da keyphrase (-1=negativo,0=neutro,1=positivo)
			"negative": [ ["str","float","int"] ], // resultados para todos domínios e com sentimento negativo
												// string do keyphrase;
												// peso do keyphrase (menos é melhor);
												// sentimento da keyphrase (-1=negativo,0=neutro,1=positivo)
		} ]

}